<!DOCTYPE html>
<html>
  <head>
    <title>Database Results</title>
  </head>
  <body>
    <h1>Database Results</h1>
    <table>
      <tr>
        <th>Order ID</th>
        <th>Scheduled Time</th>
        <th>Products</th>
        <th>Status</th>
        <th>Total Price</th>
      </tr>
      <% data.forEach(row => { %>
      <tr>
        <td><%= row.order_id %></td>
        <td><%= row.scheduled_time %></td>
        <td><%= row.products %></td>
        <td><%= row.status %></td>
        <td><%= row.total_price %></td>
      </tr>
      <% }); %>
    </table>
  </body>
</html>
